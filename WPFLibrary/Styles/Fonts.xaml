    <FontFamily x:Key="Montserrat-ExtraBold">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat ExtraBold</FontFamily>
    <FontFamily x:Key="Montserrat-ExtraBoldItalic">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat ExtraBoldItalic</FontFamily>
    
    <FontFamily x:Key="Montserrat-ExtraLight">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat ExtraLight</FontFamily>
