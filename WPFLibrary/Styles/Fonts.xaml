<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    
    <FontFamily x:Key="Montserrat-Black">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Black</FontFamily>
    <FontFamily x:Key="Montserrat-BlackItalic">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat BlackItalic</FontFamily>
    
    <FontFamily x:Key="Montserrat-Bold">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Bold</FontFamily>
