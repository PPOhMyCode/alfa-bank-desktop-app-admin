<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    
    <FontFamily x:Key="Montserrat-Bold">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Bold</FontFamily>
    <FontFamily x:Key="Montserrat-Regular">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Regular</FontFamily>
    <FontFamily x:Key="Montserrat-Medium">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Medium</FontFamily>
    <FontFamily x:Key="Montserrat-Light">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Light</FontFamily>

    <system:Double x:Key="Small">14</system:Double>
    <system:Double x:Key="Regular">18</system:Double>
    <system:Double x:Key="Big">24</system:Double>
    <system:Double x:Key="H1">32</system:Double>

</ResourceDictionary>