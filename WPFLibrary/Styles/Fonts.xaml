    <FontFamily x:Key="Montserrat-Regular">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Regular</FontFamily>
    
    <FontFamily x:Key="Montserrat-SemiBold">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat SemiBold</FontFamily>
