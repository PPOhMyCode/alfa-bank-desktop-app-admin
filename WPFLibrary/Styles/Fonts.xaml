    <FontFamily x:Key="Montserrat-Italic">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Italic</FontFamily>
    
    <FontFamily x:Key="Montserrat-Light">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Light</FontFamily>
    <FontFamily x:Key="Montserrat-LightItalic">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat LightItalic</FontFamily>
    
