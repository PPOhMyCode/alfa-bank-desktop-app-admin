    <FontFamily x:Key="Montserrat-Thin">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat Thin</FontFamily>
    <FontFamily x:Key="Montserrat-ThinItalic">pack://application:,,,/WPFLibrary;component/Fonts/#Montserrat ThinItalic</FontFamily>
    
    <system:Double x:Key="Small">14</system:Double>
    <system:Double x:Key="Regular">18</system:Double>
    <system:Double x:Key="Big">24</system:Double>
    <system:Double x:Key="H1">32</system:Double>

