    <Style x:Key="CircleDeleteDishButton" TargetType="Button">
        <Setter Property="Height" Value="45"/>
        <Setter Property="Width" Value="45"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid HorizontalAlignment="Center">
                        <Ellipse x:Name="Ellipse" 
                                 Fill="{StaticResource LightTextBrush}"
                                 Stretch="Uniform" Width="50" Height="50"/>
                        
                        <Rectangle x:Name="imageColor" Width="30" Height="30">
                            <Rectangle.Fill>
                                <SolidColorBrush x:Name="ColorBrush" Color="{StaticResource TrashIconColor}"/>
                            </Rectangle.Fill>
                            <Rectangle.OpacityMask>
                                <ImageBrush ImageSource="pack://application:,,,/Desktop-Canteen;component/Resources/TrashCanOutline.png"/>
                            </Rectangle.OpacityMask>
                        </Rectangle>
                        
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDefaulted" Value="True">
                            <Setter Property="Fill" TargetName="Ellipse" Value="{StaticResource LightTextBrush}" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <!-- <Setter TargetName="imageColor" Property="Fill" Value="{StaticResource IsMouseOverTrashIconColor}"/> -->
                            <Setter TargetName="Ellipse" Property="Fill" Value="{StaticResource IsMouthOverDeleteCircleBackgroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <!-- <Setter TargetName="imageColor" Property="Fill" Value="{StaticResource IsDisableTrashIconColor}"/> -->
                            <Setter TargetName="Ellipse" Property="Fill" Value="{StaticResource IsDisableDeleteCircleBackgroundBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <Style x:Key="CircleAddToMenuDishButton" TargetType="Button">
        <Setter Property="Height" Value="45"/>
        <Setter Property="Width" Value="45"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid HorizontalAlignment="Center">
                        <Ellipse x:Name="Ellipse" 
                                 Fill="{StaticResource LightTextBrush}"
                                 Stretch="Uniform" Width="50" Height="50"/>
                        
                        <TextBlock x:Name="TextBlock"
                                   Width="43" Height="77"
                                   HorizontalAlignment="Center"
                                   Text="+"
                                   FontSize="64"
                                   Foreground="{StaticResource GreenBrush}"
                                   VerticalAlignment="Top"
                                   Margin="0, -25, 0, 0"/>
                        
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDefaulted" Value="True">
                            <Setter Property="Fill" TargetName="Ellipse" Value="{StaticResource LightTextBrush}" />
                            <Setter TargetName="TextBlock" Property="Foreground" Value="{StaticResource GreenBrush}" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Ellipse" Property="Fill" Value="{StaticResource LightGreenBrush}" />
                            <Setter TargetName="TextBlock" Property="Foreground" Value="{StaticResource DarkGreenBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="Ellipse" Property="Fill" Value="{StaticResource IsDisableDeleteCircleBackgroundBrush}" />
                            <Setter TargetName="TextBlock" Property="Foreground" Value="{StaticResource IsDisableTrashIconBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
