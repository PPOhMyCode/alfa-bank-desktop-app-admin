    <Style x:Key="DefaultLightButton" TargetType="Button">
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Foreground" Value="{StaticResource DarkGrayBrush}"/>
        <Setter Property="Background" Value="{StaticResource CircleButtonBackgroundBrush}"/>
        <Setter Property="FontSize" Value="19"/>
        <Setter Property="FontFamily" Value="{StaticResource Montserrat-Medium}"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border
                        Name="brd"
                        BorderBrush="{StaticResource GreenBrush}"
                        Background="{StaticResource CircleButtonBackgroundBrush}"
                        CornerRadius="5"
                        BorderThickness="1.5">
                        <ContentPresenter
                            Margin="0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDefaulted" Value="True">
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="brd" Property="Background" Value="{StaticResource UltraLightGreenBrush}" />
                            <Setter Property="Foreground" Value="{StaticResource UltraDarkGreenBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter Property="Foreground" Value="{StaticResource DarkGreenBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="brd" Property="Background" Value="{StaticResource GrayBrush}" />
                            <Setter Property="Foreground" Value="{StaticResource LightTextBrush}" />
                            <Setter TargetName="brd" Property="BorderBrush" Value="{StaticResource DarkGrayBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
